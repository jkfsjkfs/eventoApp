{
  "name": "evento-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
  "start": "react-scripts start",
  "prebuild": "chmod +x node_modules/.bin/react-scripts || echo 'Skipping chmod on Windows'",
  "build": "react-scripts build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-router-dom": "^6.12.1",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "react-qr-reader": {
      "react": "$react"
    }
  },
  "engines": {
    "npm": ">=7.0.0"
  },
  "config": {
    "legacy-peer-deps": true
  }
